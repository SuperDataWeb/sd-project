        function checkData() {
            if (_Obj("ctl00_ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_ContentPlaceHolder1_txtComPanyName").value == "") {
                _Obj("spanComPanyName").innerHTML = wrong + "&nbsp;公司名称不为空!";
                return false;
            }
            else {
                _Obj("spanComPanyName").innerHTML = right;
            }
            if (_Obj("ctl00_ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_ContentPlaceHolder1_txtLinkMan").value == "") {
                _Obj("spanLinkMan").innerHTML = wrong + "&nbsp;联系人不为空!";
                return false;
            }
            else {
                _Obj("spanLinkMan").innerHTML = right;
            }
            //电话
            var phone = _Obj("ctl00_ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_ContentPlaceHolder1_txtPhone").value;
            var exec_phone = /^[0-9]{2,6}[-][0-9]{7,8}|[1][0-9]{10}$/;
            var exec_mobile = /^(13[0-9]|15[0|3|6|7|8|9]|18[8|9])[0-9]{8}$/;
            if (phone == "" || phone == null) {
                _Obj("spanPhone").innerHTML = wrong + "&nbsp;请输入电话!";
                return false;
            }
            else if (phone.indexOf('-') > 0) {
                if (!exec_phone.exec(phone)) {
                    _Obj("spanPhone").innerHTML = wrong + "&nbsp;请输入电话!";
                    return false;
                }
                else {
                    _Obj("spanPhone").innerHTML = right;
                }
            } else if (!exec_mobile.exec(phone)) {
                _Obj("spanPhone").innerHTML = wrong + "&nbsp;请输入电话!";
                return false;
            }
            else if (isNaN(phone)) {
                _Obj("spanPhone").innerHTML = wrong + "&nbsp;请输入电话!";
                return false;
            }
            else {
                _Obj("spanPhone").innerHTML = right;
            }
            //Email
            var email = _Obj("ctl00_ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_ContentPlaceHolder1_txtEmail").value;
            var exec_email = /^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/;
            if (!exec_email.exec(email)) {
                _Obj("spanEmail").innerHTML = wrong + "&nbsp;Email不为空!";
                return false;
            }
            else {
                _Obj("spanEmail").innerHTML = right;
            }
            return true;
        }
    </SCRIPT>

